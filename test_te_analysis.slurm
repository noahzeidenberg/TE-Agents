# Source the tools configuration
source /scratch/$USER/tools/tools_config.sh

# Create test directory
TEST_DIR="te_test"
mkdir -p $TEST_DIR
cd $TEST_DIR

# Extract chromosome III
python ../extract_chr3.py

# Create a custom library for yeast TEs
cat > yeast_te.lib << EOF
>Ty1 
 