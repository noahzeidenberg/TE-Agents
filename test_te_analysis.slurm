#!/bin/bash
#SBATCH --account=def-lukens
#SBATCH --time=1:00:00
#SBATCH --cpus-per-task=4
#SBATCH --mem=16000M
#SBATCH --output=%x-%j.out

# Load required modules
module purge
module load StdEnv/2020
module load gcc/9.3.0
module load python
module load blast+
module load hmmer
module load trf
